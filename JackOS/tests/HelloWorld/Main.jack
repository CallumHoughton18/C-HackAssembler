/** Test program for the OS Output class. */
class Main {

    /** Outputs the entire character set to the screen using all the
     *  methods of the Output class. */
    function void main() {
        var String usersName;
        
        let usersName = Keyboard.readLine("Type your name: ");

        do Output.printString("Hello, ");
        do Output.printString(usersName);
        do Output.printString(", from Jack to Hack!");
        
        return;
    }
}
